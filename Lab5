TASK: 1

.model small

.stack 100h

.data

.code

main proc

mov ah, 1h         

int 21h             

sub al,30h        

mov bl, al         

mov ah,1h        

int 21h            

sub al,30h        

mov dl, al         

add dl,30h      

mov ah, 2h        

int 21h             

add bl,30h      

mov dl, bl          

mov ah, 2h        

int 21h             

mov ah, 4Ch         

int 21h           

main endp

end main

